{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "cross-env DATABASE_URL=\"file:./prisma/dev.db\" prisma migrate deploy --schema prisma/schema.prisma && cross-env DATABASE_URL=\"file:./prisma/dev.db\" prisma generate && cross-env DATABASE_URL=\"file:./prisma/dev.db\" jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "prisma:generate": "cross-env DATABASE_URL=\"file:./prisma/dev.db\" prisma generate",
    "prisma:migrate": "cross-env DATABASE_URL=\"file:./prisma/dev.db\" prisma migrate dev",
    "prisma:deploy": "cross-env DATABASE_URL=\"file:./prisma/dev.db\" prisma migrate deploy",
    "prisma:seed": "cross-env DATABASE_URL=\"file:./prisma/dev.db\" ts-node --project tsconfig.json prisma/seed.ts",
    "agent:demo": "tsx src/agent/demoRunner.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.7",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "pino": "^8.17.0",
    "pino-http": "^8.5.1",
    "@solana/web3.js": "^1.91.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.1.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

